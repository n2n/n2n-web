{
	"name" : "n2n/n2n-web",
	"type" : "n2n-module",
	"description" : "n2n web",
	"keywords" : [
		"n2n",
		"rest",
		"controler",
		"http",
		"form api"
	],
	"homepage" : "https://n2n.rocks/",
	"license" : "LGPL-3.0-or-later",
	"authors" : [{
			"name" : "Andreas von Burg",
			"email" : "a@von-burg.net",
			"homepage" : "https://www.von-burg.net/",
			"role" : "Developer"
		}
	],
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"config" : {
		"vendor-dir" : "src/vendor",
		"allow-plugins": {
			"n2n/n2n-composer-module-installer": true
		}
	},
	"require" : {
		"n2n/n2n" : "~7.4",
		"n2n/n2n-context" : "~7.4",
		"n2n/n2n-l10n" : "~7.4",
		"n2n/n2n-reflection" : "~7.4",
		"n2n/n2n-util" : "~7.4",
		"psr/http-factory": "^1",
		"psr/http-message": "^1"
    },
	"autoload" : {
		"psr-4" : {
			"n2n\\web\\" : "src/app/n2n/web"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"n2n\\web\\" : "src/test/n2n/web"
		}
	},
	"require-dev" : {
		"phpunit/phpunit" : "^9.5"
	}
}
